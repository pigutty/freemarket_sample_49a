$(document).on('turbolinks:load', function(){
  $('.slider').slick({
    dots: true,
    prevArrow:'<%= image_tag "prev.png",class: "slick-prev"%>',
    nextArrow:'<%= image_tag "next.png",class:"slick-next"%>'
  });
  $('.slick-dots').find('li').on('mouseover', function() {
    $(this).click();
  });
});